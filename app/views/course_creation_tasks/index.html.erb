<div class="page-header ">
  <h1 class="text-center">Course Creation Tasks</h1>
  <div class="text-center">
  </br>
     <%= link_to 'View Courses | Add Task', empire_courses_path, class: 'btn btn-primary' %>
  </br>
  </div>
</div>
</br>


<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Status</th>
      <th>Course ID</th>
      <th>Title</th>
      <th>Job ID</th>
      <th>Job</th>
      <th>Template</th>
      <th>Instructions</th>
      <th>Task note 1</th>
      <!-- <th>Task note 3</th> -->
      <th>Assign 1</th>
      <!-- <th>Assign 2</th> -->
      <!-- <th>Assign 3</th> -->
      <!-- <th>Extra s</th>
      <th>Extra t</th>
      <th>Extra i</th>
      <th>Extra d</th>
      <th>Extra b</th> -->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_creation_tasks.each do |course_creation_task| %>
      <tr>
        <td><%= course_creation_task.extra_s.blank? ? 'Not Taken' : course_creation_task.extra_s %></td>
        <td><%= course_creation_task.empire_course_id %></td>

      </tr>
    <% end %>
  </tbody>
</table>

