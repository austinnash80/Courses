          <th>List</th>
          <th>Count</th>
        </tr>
      </thead>
      <tbody>
        <% @states.each do |k,v| %>
        <tr>
          <td><%= k %></td>
          <td><%= number_with_delimiter(v.count) %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>

