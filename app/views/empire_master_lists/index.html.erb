<p id="notice"><%= notice %></p>

<div class="well">
  <h1 class="text-center">Empire Master Lists</h1>
</div>


<h3>Upload CSV File</h3>
  <%= form_tag import_empire_master_lists_path, multipart: true do %>
  <%= file_field_tag :file %>
<br>
  <%= submit_tag 'Upload', class: 'btn btn-primary' %>
<% end %>
<br>

<div class="row">
  <div class="col-md-3">
    <table class="table table-outline">
      <thead>
        <tr>
          <th>List</th>
          <th>Count</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td><%= @ca[0] %></td>
          <td><%= @ca.count %></td>
        </tr>
        <tr>
          <td><%= @ny[0] %></td>
          <td><%= @ny.count %></td>
        </tr>
        <tr>
          <td><%= @nm[0] %></td>
          <td><%= @nm.count %></td>
        </tr>
        <tr>
          <td><%= @ut[0] %></td>
          <td><%= @ut.count %></td>
        </tr>
        <tr>
          <td><%= @wa[0] %></td>
          <td><%= @wa.count %></td>
        </tr>

        <%# @states.each do |k,v| %>
        <!-- <tr> -->
          <!-- <td><%#= k %></td> -->
          <!-- <td><%#= number_with_delimiter(v.count) %></td> -->
        <!-- </tr> -->
        <%# end %>
      </tbody>
    </table>
  </div>
</div>
