
<%= simple_form_for(@empire_master_match) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :uid %>
    <%= f.input :last_name %>
    <%= f.input :list %>
    <%= f.input :lid %>
    <%= f.input :search_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
