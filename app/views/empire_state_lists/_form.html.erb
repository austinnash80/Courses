

<%= simple_form_for(@empire_state_list) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :st %>
    <%= f.input :tilte %>
    <%= f.input :cost %>
    <%= f.input :notes %>
    <%= f.input :list_upload, as: :file %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
