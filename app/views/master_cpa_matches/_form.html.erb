
<%= simple_form_for(@master_cpa_match) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :uid %>
    <%= f.input :lname %>
    <%= f.input :list %>
    <%= f.input :search_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
