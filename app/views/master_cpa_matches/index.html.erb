<p id="notice"><%= notice %></p>

<h1 class="text-center">Master Cpa Matches</h1>

<table class="table table-outline">
  <caption>Displaying 250 of <%= number_with_delimiter(@master_cpa_matches.count) %></caption>
  <thead>
    <tr>
      <th>Uid</th>
      <th>Lname</th>
      <th>lid</th>
      <th>List</th>
      <th>Search date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% MasterCpaMatch.order(search_date: :desc).first(250).each do |master_cpa_match| %>
      <tr>
        <td><%= master_cpa_match.uid %></td>
        <td><%= master_cpa_match.lname %></td>
        <td><%= master_cpa_match.lid %></td>
        <td><%= master_cpa_match.list %></td>
        <td><%= master_cpa_match.search_date %></td>
        <td><%= link_to 'Show', master_cpa_match %></td>
        <td><%= link_to 'Edit', edit_master_cpa_match_path(master_cpa_match) %></td>
        <td><%= link_to 'Destroy', master_cpa_match, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Master Cpa Match', new_master_cpa_match_path %>
