<div class="well jumbotron text-center">
  <h1>Postcard Returns - Stats</h1>
</div>

<div class="col-md-3">
  <h2 class="text-center">Sequoia </h2>
  <table class="table table-hover table-bordered">
    <thead>
      <tr>
        <th style="width: 75%">Type</th>
        <th>Number</th>
      </tr>
    </thead>
    <tbody id="">
      <% @sequoia_postcard_retun_total.each do |k, v| %>
      <tr>
        <td><%= k %></td>
        <td><%= v %></td>
      </tr>
      <% end %>
    </tbody>
  </table>

  <table class="table table-hover table-bordered">
